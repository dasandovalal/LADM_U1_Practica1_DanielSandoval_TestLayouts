<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="DISQUERA"
        android:textSize="20sp"
        android:id="@+id/diqueratext"
        />
    <ImageView
        android:id="@+id/logocactus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cactuslogo"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:layout_below="@id/diqueratext"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/logocactus"
        android:textSize="30sp"
        android:id="@+id/records"
        android:text="EL CACTUS RECORDS"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/records"
        android:layout_marginTop="50dp"
        android:textSize="20sp"
        android:text="Â¡BIENVENIDOS!"/>



</RelativeLayout>